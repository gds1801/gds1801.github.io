			<h2 style="width: 50%;" class="section-title">Расписание</h2>
			<div class="aux-nav">
 <a href="/schedule/"> <i class="fa fa-paperclip"></i> Всё расписание</a>
			</div>
		</div>
		 <script type="text/javascript">
  $(function(){
    $("#section").on("change",function(){
      $("#element [value='']").attr("selected", "selected");
      if($("#section").val()!="")
      {
        $("#element").show();
        $('[data-class]').hide();
        qw = $("#section").val();
        $('[data-class="'+qw+'"]').show();
      }
      else $("#element").hide();
    }
                    );
    $("#element").on("change",function(){
      if($("#element").val()!=""){
        element=$("#element").val();
        $.ajax(
			{type: "POST",
			url: "/ajax/schedule.php",
			data: "id="+element,
			success: function(data) {
			$("#currentschedule").html(data);}
});
      }
    }
                    );
  }
   );
</script>
